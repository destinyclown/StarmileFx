                            <div style="width: 20%; text-align: center; float: left;">
                                <span id="e_value"></span>
                                <p style="color: #a1a1a1">Safari</p>
                            </div>
                            <div style="width: 20%; text-align: center; float: left;">
                                <span id="c_value"></span>
